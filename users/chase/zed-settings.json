{
    "assistant": {
        "default_model": {
            "model": "claude-3-5-sonnet-latest",
            "provider": "zed.dev"
        },
        "version": "2"
    },
    "auto_install_extensions": {},
    "auto_update": false,
    "autosave": "on_window_change",
    "base_keymap": "JetBrains",
    "buffer_font_size": 18,
    "format_on_save": "on",
    "git": {
        "inline_blame": {
            "enabled": false
        }
    },
    "inlay_hints": {
        "enabled": true,
        "show_type_hints": false
    },
    "languages": {
        "YAML": {
            "tab_size": 2
        }
    },
    "lsp": {
        "rust-analyzer": {
            "initialization_options": {
                "check": {
                    "command": "clippy"
                }
            }
        }
    },
    "scroll_sensitivity": 1,
    "soft_wrap": "editor_width",
    "tab_size": 4,
    "theme": "Ayu Mirage",
    "ui_font_size": 22
}
