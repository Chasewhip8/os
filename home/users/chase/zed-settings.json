{
    "disable_ai": false,
    "notification_panel": {
        "button": false
    },
    "collaboration_panel": {
        "button": false
    },
    "outline_panel": {
        "button": false
    },
    "edit_predictions": {
        "mode": "subtle",
        "copilot": {
            "proxy": null,
            "proxy_no_verify": null
        },
        "enabled_in_text_threads": false
    },
    "features": {
        "edit_prediction_provider": "zed"
    },
    "agent": {
        "button": true,
        "always_allow_tool_actions": false,
        "default_profile": "write",
        "default_view": "text_thread",
        "profiles": {
            "write": {
                "name": "Write",
                "tools": {
                    "batch_tool": true,
                    "code_actions": false,
                    "code_symbols": true,
                    "contents": false,
                    "copy_path": false,
                    "create_file": true,
                    "delete_path": false,
                    "diagnostics": true,
                    "edit_file": true,
                    "fetch": true,
                    "list_directory": true,
                    "move_path": false,
                    "now": false,
                    "find_path": true,
                    "read_file": true,
                    "grep": true,
                    "rename": false,
                    "symbol_info": true,
                    "terminal": true,
                    "thinking": true,
                    "web_search": true
                },
                "enable_all_context_servers": true,
                "context_servers": {}
            }
        },
        "inline_assistant_model": {
            "provider": "google",
            "model": "gemini-2.5-flash-preview-04-17"
        },
        "default_model": {
            "provider": "zed.dev",
            "model": "claude-sonnet-4"
        }
    },
    "auto_update": true,
    "base_keymap": "JetBrains",
    "theme": "Ayu Mirage",
    "soft_wrap": "editor_width",
    "ui_font_size": 20,
    "buffer_font_size": 17,
    "agent_ui_font_size": 19,
    "scroll_sensitivity": 1,
    "autosave": "on_window_change",
    "tab_size": 4,
    "format_on_save": "on",
    "lsp": {
        "rust-analyzer": {
            "initialization_options": {
                "check": {
                    "command": "clippy"
                }
            }
        },
        "biome": {
            "settings": {
                "require_config_file": true
            }
        },
        "vtsls": {
            "settings": {
                "typescript": {
                    "tsserver": {
                        "experimental": {
                            "enableProjectDiagnostics": false
                        }
                    },
                    "updateImportsOnFileMove": {
                        "enabled": "always"
                    }
                }
            }
        }
    },
    "git": {
        "inline_blame": {
            "enabled": false
        }
    },
    "inlay_hints": {
        "enabled": false
    }
}
